    "import neattext as nt \n",
    "import neattext.functions as nfx\n",
    "\n",
    "def neatText(df):\n",
    "    #Remove user handles, hastags, emojis, emails, urls and html balises\n",
    "    df['cleanText'] = df['body'].apply(nfx.remove_hashtags)\n",
    "    df['cleanText'] = df['cleanText'].apply(nfx.remove_userhandles)\n",
    "    df['cleanText'] = df['cleanText'].apply(nfx.remove_emails)\n",
    "    df['cleanText'] = df['cleanText'].apply(nfx.remove_urls)\n",
    "    df['cleanText'] = df['cleanText'].apply(nfx.remove_emojis)\n",
    "    return df"
   ]
