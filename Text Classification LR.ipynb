    "def eval_dfs_preprocess(df,twitter):\n",
    "    \n",
    "    #drop duplicates\n",
    "    df.drop_duplicates(subset='body', keep= 'first', inplace=True)\n",
    "    \n",
    "    #Drop null rows\n",
    "    df = df.dropna(how='any',axis=0) \n",
    "    \n",
    "    #take a sample\n",
    "    if twitter: #for twitter sample take all the data, for the other sets take a sample\n",
    "        \n",
    "        df = df.sample(n=10000, random_state=random.randint(0,100))\n",
    "    \n",
    "   \n",
    "    \n",
    "    #Remove user handles, hastags, emojis, emails, urls and html balises\n",
    "    df = neatText(df) #removing emojis,urls,hashtags\n",
    "    \n",
    "    df['cleanText'] = df['cleanText'].apply(clean_text)\n",
    "    \n",
    "    return df"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 44,
