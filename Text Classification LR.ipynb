   "outputs": [],
   "source": [
    "\n",
    "\n",
    "def model_eval_on_subreddits(evaluation_dfs,model,evaluation_dfs_names):\n",
    "    dfWrongLabelEval = {}\n",
    "    for i,df in enumerate(evaluation_dfs):\n",
    "        print(50*'#')\n",
    "        print(evaluation_dfs_names[i])\n",
    "        df = eval_dfs_preprocess(df,True)\n",
    "        \n",
    "        #apply the model and make a new column for the predictions\n",
    "        df['Predicted_label'] = df['cleanText'].apply(predict_label,model=lr_model)\n",
    "        \n",
    "        #print examples of wrong labels\n",
    "        filtered_data = df[df[\"Predicted_label\"]==1]\n",
    "        print(filtered_data['body'].iloc[0:10])\n",
    "        \n",
    "        #calculate the percentage of wrong labeling (parler)\n",
    "        redditLabeled = (df.Predicted_label.values == 0).sum()\n",
    "        parlerLabeled = (df.Predicted_label.values == 1).sum()\n",
    "        \n",
    "        wrongLabelPercentage = (parlerLabeled/df.shape[0])*100\n",
    "        wrongLabelPercentage = float(\"{:.2f}\".format(wrongLabelPercentage))\n",
    "        \n",
    "        dfWrongLabelEval[evaluation_dfs_names[i]] = wrongLabelPercentage\n",
    "    \n",
    "    return dfWrongLabelEval"
   ]
  },
